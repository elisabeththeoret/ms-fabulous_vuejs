{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport PastryDataService from '@/services/PastryDataService';\nexport default {\n  props: ['inventory', 'updateInv', 'removeInv', 'removeItem'],\n  data() {\n    return {\n      pastry: {},\n      id: parseInt(this.$route.params.id),\n      submitted: false\n      // message: null\n    };\n  },\n\n  methods: {\n    // update\n    updatePastry() {\n      PastryDataService.update(this.id, this.pastry).then(res => {\n        this.updateInv(this.pastryIndex, this.pastry);\n        // success message\n        this.submitted = true;\n      });\n    },\n    // delete\n    deletePastry() {\n      PastryDataService.delete(this.id).then(res => {\n        // remove from cart\n        this.removeItem(this.pastry.name);\n        // delete from dom\n        this.removeInv(this.pastryIndex);\n        // redirect\n        this.$router.push({\n          name: 'home'\n        });\n      });\n    }\n  },\n  computed: {\n    // find pastry index\n    pastryIndex() {\n      const index = this.inventory.findIndex(p => {\n        return p.id === this.id;\n      });\n      return index;\n    }\n  },\n  // get data by id\n  mounted() {\n    PastryDataService.get(this.id).then(res => {\n      this.pastry = res.data;\n    });\n  }\n};","map":{"version":3,"names":["PastryDataService","props","data","pastry","id","parseInt","$route","params","submitted","methods","updatePastry","update","then","res","updateInv","pastryIndex","deletePastry","delete","removeItem","name","removeInv","$router","push","computed","index","inventory","findIndex","p","mounted","get"],"sources":["/Users/elisabeththeoret/Desktop/_PROG_WEB/_ALL_SESSION/582-41F_tech_avancees_prog/all_evalue/TP03/ms-fabulous_vuejs/frontend/src/views/EditPastryView.vue"],"sourcesContent":["<template>\n  <div class=\"pastry-add\">\n    <!-- Section-->\n    <section class=\"py-5\">\n      <div class=\"container px-4 px-lg-5 mt-5\">\n        <div class=\"row gx-4 gx-lg-5 row-cols-2 row-cols-md-3 row-cols-xl-2 justify-content-center\">\n          <div class=\"col-sm-12\">\n            <h4 class=\"mb-3\">Edit pastry</h4>\n            <div class=\"alert alert-danger\" v-show=\"message\">{{ message }}</div>\n            <div class=\"needs-validation\" novalidate>\n              <div class=\"row g-2\">\n                <div v-if=\"!submitted\">\n                  <div class=\"col-12\">\n                    <label for=\"pastryName\" class=\"form-label\">Pastry Name</label>\n                    <input type=\"text\" class=\"form-control\" id=\"pastryName\" placeholder=\"\" v-model=\"pastry.name\" required />\n                    <div class=\"invalid-feedback\">Valid name is required.</div>\n                  </div>\n                  <div class=\"col-12\">\n                    <label for=\"pastryPhoto\" class=\"form-label\">Pastry Photo</label>\n                    <input type=\"text\" class=\"form-control\" id=\"pastryPhoto\" placeholder=\"\" v-model=\"pastry.photo\" required />\n                    <div class=\"invalid-feedback\">Valid photo path is required.</div>\n                  </div>\n                  <div class=\"col-12\">\n                    <label for=\"pastryPrice\" class=\"form-label\">Price</label>\n                    <div class=\"input-group has-validation\">\n                      <span class=\"input-group-text\">CAD</span>\n                      <input type=\"text\" class=\"form-control\" id=\"pastryPrice\" placeholder=\"\" v-model.number=\"pastry.price\" required />\n                      <div class=\"invalid-feedback\">Price is required.</div>\n                    </div>\n                  </div>\n                  <div class=\"col-12\">\n                    <label for=\"pastryDescription\" class=\"form-label\">Pastry Description</label>\n                    <textarea class=\"form-control\" id=\"pastryDescription\" placeholder=\"\" v-model=\"pastry.description\"></textarea>\n                    <div class=\"invalid-feedback\">Valid description.</div>\n                  </div>\n                  <div class=\"col-12\">\n                    <label for=\"pastryType\" class=\"form-label\">Pastry Type</label>\n                    <select class=\"form-control\" id=\"pastryType\" placeholder=\"\" v-model=\"pastry.type\" required>\n                      <option value=\"\">Select</option>\n                      <option value=\"Burguer\">Burger</option>\n                      <option value=\"Italian\">Italian</option>\n                      <option value=\"Indian\">Indian</option>\n                      <option value=\"Thai\">Thai</option>\n                    </select>\n                    <div class=\"invalid-feedback\">Valid photo path is required.</div>\n                  </div>\n                </div>\n                <div v-else>\n                  <div class=\"alert alert-success alert-dismissible fade show\" role=\"alert\">\n                    <strong>You submitted successfully!</strong>\n                    <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"alert\" aria-label=\"Close\"></button>\n                  </div>\n                  <button class=\"w-100 btn btn-success btn-lg mt-3\" type=\"button\" @click=\"editPastry\">Edit pastry</button>\n                </div>\n                <hr class=\"my-4\">\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n  </div>\n  <button class=\"w-100 btn btn-primary btn-lg mt-3\" type=\"button\" @click=\"updatePastry\">Update</button>\n  <button class=\"w-100 btn btn-danger btn-lg mt-3\" type=\"button\" @click=\"deletePastry\">Delete</button>\n</template>\n\n<script>\nimport PastryDataService from '@/services/PastryDataService'\n\nexport default {\n\n  props: ['inventory', 'updateInv', 'removeInv', 'removeItem'],\n\n  data () {\n    return {\n      pastry: {},\n      id: parseInt(this.$route.params.id),\n      submitted: false\n      // message: null\n    }\n  },\n\n  methods: {\n    // update\n    updatePastry () {\n      PastryDataService.update(this.id, this.pastry).then(res => {\n        this.updateInv(this.pastryIndex, this.pastry)\n        // success message\n        this.submitted = true\n      })\n    },\n\n    // delete\n    deletePastry () {\n      PastryDataService.delete(this.id).then(res => {\n        // remove from cart\n        this.removeItem(this.pastry.name)\n        // delete from dom\n        this.removeInv(this.pastryIndex)\n        // redirect\n        this.$router.push({ name: 'home' })\n      })\n    }\n  },\n\n  computed: {\n    // find pastry index\n    pastryIndex () {\n      const index = this.inventory.findIndex((p) => {\n        return p.id === this.id\n      })\n      return index\n    }\n  },\n\n  // get data by id\n  mounted () {\n    PastryDataService.get(this.id).then(res => {\n      this.pastry = res.data\n    })\n  }\n\n}\n</script>\n"],"mappings":";AAmEA,OAAOA,iBAAgB,MAAO,8BAA6B;AAE3D,eAAe;EAEbC,KAAK,EAAE,CAAC,WAAW,EAAE,WAAW,EAAE,WAAW,EAAE,YAAY,CAAC;EAE5DC,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,MAAM,EAAE,CAAC,CAAC;MACVC,EAAE,EAAEC,QAAQ,CAAC,IAAI,CAACC,MAAM,CAACC,MAAM,CAACH,EAAE,CAAC;MACnCI,SAAS,EAAE;MACX;IACF;EACF,CAAC;;EAEDC,OAAO,EAAE;IACP;IACAC,YAAWA,CAAA,EAAK;MACdV,iBAAiB,CAACW,MAAM,CAAC,IAAI,CAACP,EAAE,EAAE,IAAI,CAACD,MAAM,CAAC,CAACS,IAAI,CAACC,GAAE,IAAK;QACzD,IAAI,CAACC,SAAS,CAAC,IAAI,CAACC,WAAW,EAAE,IAAI,CAACZ,MAAM;QAC5C;QACA,IAAI,CAACK,SAAQ,GAAI,IAAG;MACtB,CAAC;IACH,CAAC;IAED;IACAQ,YAAWA,CAAA,EAAK;MACdhB,iBAAiB,CAACiB,MAAM,CAAC,IAAI,CAACb,EAAE,CAAC,CAACQ,IAAI,CAACC,GAAE,IAAK;QAC5C;QACA,IAAI,CAACK,UAAU,CAAC,IAAI,CAACf,MAAM,CAACgB,IAAI;QAChC;QACA,IAAI,CAACC,SAAS,CAAC,IAAI,CAACL,WAAW;QAC/B;QACA,IAAI,CAACM,OAAO,CAACC,IAAI,CAAC;UAAEH,IAAI,EAAE;QAAO,CAAC;MACpC,CAAC;IACH;EACF,CAAC;EAEDI,QAAQ,EAAE;IACR;IACAR,WAAUA,CAAA,EAAK;MACb,MAAMS,KAAI,GAAI,IAAI,CAACC,SAAS,CAACC,SAAS,CAAEC,CAAC,IAAK;QAC5C,OAAOA,CAAC,CAACvB,EAAC,KAAM,IAAI,CAACA,EAAC;MACxB,CAAC;MACD,OAAOoB,KAAI;IACb;EACF,CAAC;EAED;EACAI,OAAMA,CAAA,EAAK;IACT5B,