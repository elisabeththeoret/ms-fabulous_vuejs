{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport ProductDataService from '@/services/ProductDataService';\nexport default {\n  props: ['inventory', 'removeInv'],\n  data() {\n    return {\n      product: {},\n      id: parseInt(this.$route.params.id),\n      submitted: false,\n      message: null\n    };\n  },\n  methods: {\n    // update\n    updateProduct() {\n      ProductDataService.update(this.id, this.product).then(res => {\n        this.submitted = true;\n      });\n    },\n    // delete\n    deleteProduct() {\n      ProductDataService.delete(this.id).then(res => {\n        // delete from dom\n        this.removeInv(this.productIndex);\n        // redirect\n        this.$router.push({\n          name: 'home'\n        });\n      });\n    }\n  },\n  computed: {\n    // find product index\n    productIndex() {\n      const index = this.inventory.findIndex(p => {\n        return p.id === this.id;\n      });\n      return index;\n    }\n  },\n  // get data by id\n  mounted() {\n    ProductDataService.get(this.id).then(res => {\n      this.product = res.data;\n    });\n  }\n};","map":{"version":3,"names":["ProductDataService","props","data","product","id","parseInt","$route","params","submitted","message","methods","updateProduct","update","then","res","deleteProduct","delete","removeInv","productIndex","$router","push","name","computed","index","inventory","findIndex","p","mounted","get"],"sources":["/Users/e2196008/Documents/582-41F_techniques_avancees_prog_web/cours/node/vue-server-client/project-vue/src/views/EditProductView.vue"],"sourcesContent":["<template>\n  <div class=\"product-add\">\n    <!-- Section-->\n    <section class=\"py-5\">\n      <div class=\"container px-4 px-lg-5 mt-5\">\n        <div class=\"row gx-4 gx-lg-5 row-cols-2 row-cols-md-3 row-cols-xl-2 justify-content-center\">\n          <div class=\"col-sm-12\">\n            <h4 class=\"mb-3\">Edit product</h4>\n            <div class=\"alert alert-danger\" v-show=\"message\">{{ message }}</div>\n            <div class=\"needs-validation\" novalidate>\n              <div class=\"row g-2\">\n                <div v-if=\"!submitted\">\n                  <div class=\"col-12\">\n                    <label for=\"productName\" class=\"form-label\">Product Name</label>\n                    <input type=\"text\" class=\"form-control\" id=\"productName\" placeholder=\"\" v-model=\"product.name\" required />\n                    <div class=\"invalid-feedback\">Valid name is required.</div>\n                  </div>\n                  <div class=\"col-12\">\n                    <label for=\"productPhoto\" class=\"form-label\">Product Photo</label>\n                    <input type=\"text\" class=\"form-control\" id=\"productPhoto\" placeholder=\"\" v-model=\"product.photo\" required />\n                    <div class=\"invalid-feedback\">Valid photo path is required.</div>\n                  </div>\n                  <div class=\"col-12\">\n                    <label for=\"productPrice\" class=\"form-label\">Price</label>\n                    <div class=\"input-group has-validation\">\n                      <span class=\"input-group-text\">CAD</span>\n                      <input type=\"text\" class=\"form-control\" id=\"productPrice\" placeholder=\"\" v-model.number=\"product.price\" required />\n                      <div class=\"invalid-feedback\">Price is required.</div>\n                    </div>\n                  </div>\n                  <div class=\"col-12\">\n                    <label for=\"productDescription\" class=\"form-label\">Product Description</label>\n                    <textarea class=\"form-control\" id=\"productDescription\" placeholder=\"\" v-model=\"product.description\"></textarea>\n                    <div class=\"invalid-feedback\">Valid description.</div>\n                  </div>\n                  <div class=\"col-12\">\n                    <label for=\"productType\" class=\"form-label\">Product Type</label>\n                    <select class=\"form-control\" id=\"productType\" placeholder=\"\" v-model=\"product.type\" required>\n                      <option value=\"\">Select</option>\n                      <option value=\"Burguer\">Burger</option>\n                      <option value=\"Italian\">Italian</option>\n                      <option value=\"Indian\">Indian</option>\n                      <option value=\"Thai\">Thai</option>\n                    </select>\n                    <div class=\"invalid-feedback\">Valid photo path is required.</div>\n                  </div>\n                  <button class=\"w-100 btn btn-primary btn-lg mt-3\" type=\"button\" @click=\"updateProduct\">Update</button>\n                  <button class=\"w-100 btn btn-danger btn-lg mt-3\" type=\"button\" @click=\"deleteProduct\">Delete</button>\n                </div>\n                <div v-else>\n                  <div class=\"alert alert-success alert-dismissible fade show\" role=\"alert\">\n                    <strong>You submitted successfully!</strong>\n                    <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"alert\" aria-label=\"Close\"></button>\n                  </div>\n                  <button class=\"w-100 btn btn-success btn-lg mt-3\" type=\"button\" @click=\"editProduct\">Edit product</button>\n                </div>\n                <hr class=\"my-4\">\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n  </div>\n</template>\n\n<script>\nimport ProductDataService from '@/services/ProductDataService'\n\nexport default {\n\n  props: ['inventory', 'removeInv'],\n\n  data () {\n    return {\n      product: {},\n      id: parseInt(this.$route.params.id),\n      submitted: false,\n      message: null\n    }\n  },\n\n  methods: {\n    // update\n    updateProduct () {\n      ProductDataService.update(this.id, this.product).then(res => {\n        this.submitted = true\n      })\n    },\n\n    // delete\n    deleteProduct () {\n      ProductDataService.delete(this.id).then(res => {\n        // delete from dom\n        this.removeInv(this.productIndex)\n        // redirect\n        this.$router.push({ name: 'home' })\n      })\n    }\n  },\n\n  computed: {\n    // find product index\n    productIndex () {\n      const index = this.inventory.findIndex((p) => {\n        return p.id === this.id\n      })\n      return index\n    }\n  },\n\n  // get data by id\n  mounted () {\n    ProductDataService.get(this.id).then(res => {\n      this.product = res.data\n    })\n  }\n\n}\n</script>\n"],"mappings":";AAmEA,OAAOA,kBAAiB,MAAO,+BAA8B;AAE7D,eAAe;EAEbC,KAAK,EAAE,CAAC,WAAW,EAAE,WAAW,CAAC;EAEjCC,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,OAAO,EAAE,CAAC,CAAC;MACXC,EAAE,EAAEC,QAAQ,CAAC,IAAI,CAACC,MAAM,CAACC,MAAM,CAACH,EAAE,CAAC;MACnCI,SAAS,EAAE,KAAK;MAChBC,OAAO,EAAE;IACX;EACF,CAAC;EAEDC,OAAO,EAAE;IACP;IACAC,aAAYA,CAAA,EAAK;MACfX,kBAAkB,CAACY,MAAM,CAAC,IAAI,CAACR,EAAE,EAAE,IAAI,CAACD,OAAO,CAAC,CAACU,IAAI,CAACC,GAAE,IAAK;QAC3D,IAAI,CAACN,SAAQ,GAAI,IAAG;MACtB,CAAC;IACH,CAAC;IAED;IACAO,aAAYA,CAAA,EAAK;MACff,kBAAkB,CAACgB,MAAM,CAAC,IAAI,CAACZ,EAAE,CAAC,CAACS,IAAI,CAACC,GAAE,IAAK;QAC7C;QACA,IAAI,CAACG,SAAS,CAAC,IAAI,CAACC,YAAY;QAChC;QACA,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;UAAEC,IAAI,EAAE;QAAO,CAAC;MACpC,CAAC;IACH;EACF,CAAC;EAEDC,QAAQ,EAAE;IACR;IACAJ,YAAWA,CAAA,EAAK;MACd,MAAMK,KAAI,GAAI,IAAI,CAACC,SAAS,CAACC,SAAS,CAAEC,CAAC,IAAK;QAC5C,OAAOA,CAAC,CAACtB,EAAC,KAAM,IAAI,CAACA,EAAC;MACxB,CAAC;MACD,OAAOmB,KAAI;IACb;EACF,CAAC;EAED;EACAI,OAAMA,CAAA,EAAK;IACT3B,kBAAkB,CAAC4B,GAAG,CAAC,IAAI,CAACxB,EAAE,CAAC,CAACS,IAAI,CAACC,GAAE,IAAK;MAC1C,IAAI,CAACX,OAAM,GAAIW,GAAG,CAACZ,IAAG;IACxB,CAAC;EACH;AAEF"},"metadata":{},"sourceType":"module","externalDependencies":[]}