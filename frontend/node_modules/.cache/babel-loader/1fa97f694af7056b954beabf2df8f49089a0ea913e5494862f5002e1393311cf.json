{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport PastryDataService from '@/services/PastryDataService';\nexport default {\n  props: ['inventory', 'updateInv', 'removeInv', 'removeItem'],\n  data() {\n    return {\n      pastry: {},\n      id: parseInt(this.$route.params.id),\n      submitted: false\n      // message: null\n    };\n  },\n\n  methods: {\n    // update\n    updatePastry() {\n      PastryDataService.update(this.id, this.pastry).then(res => {\n        this.updateInv(this.pastryIndex, this.pastry);\n        // success message\n        this.submitted = true;\n      });\n    },\n    // delete\n    deletePastry() {\n      PastryDataService.delete(this.id).then(res => {\n        // remove from cart\n        this.removeItem(this.pastry.name);\n        // delete from dom\n        this.removeInv(this.pastryIndex);\n        // redirect\n        this.$router.push({\n          name: 'home'\n        });\n      });\n    }\n  },\n  computed: {\n    // find pastry index\n    pastryIndex() {\n      const index = this.inventory.findIndex(p => {\n        return p.id === this.id;\n      });\n      return index;\n    }\n  },\n  // get data by id\n  mounted() {\n    PastryDataService.get(this.id).then(res => {\n      this.pastry = res.data;\n    });\n  }\n};","map":{"version":3,"names":["PastryDataService","props","data","pastry","id","parseInt","$route","params","submitted","methods","updatePastry","update","then","res","updateInv","pastryIndex","deletePastry","delete","removeItem","name","removeInv","$router","push","computed","index","inventory","findIndex","p","mounted","get"],"sources":["/Users/elisabeththeoret/Desktop/_PROG_WEB/_ALL_SESSION/582-41F_tech_avancees_prog/all_evalue/TP03/ms-fabulous_vuejs/frontend/src/views/EditPastryView.vue"],"sourcesContent":["<template>\n  <!-- Header Page -->\n  <header class=\"flex col\">\n    <h1>Mettre à jour</h1>\n    <p>Remplissez le formulaire ci-dessous afin de mettre à jour la pâtisserie : {{ pastry.name }}.</p>\n  </header>\n  <!-- Form -->\n  <form class=\"flex col gap-petit bg-light_marron\">\n    <div class=\"boite bg-light_rosemauve border-light_rosemauve\" v-show=\"message\" v-if=\"\">{{ message }}</div>\n    <div class=\"grille-lg gap-moyen\">\n      <div class=\"flex col\">\n        <div class=\"form-control\">\n          <label for=\"name\">Nom de la pâtisserie*</label>\n          <input type=\"text\" name=\"name\" id=\"name\" v-model=\"pastry.name\" />\n        </div>\n        <div class=\"form-control\">\n          <label for=\"category\">Catégorie</label>\n          <input type=\"text\" name=\"category\" id=\"category\" v-model=\"pastry.category\" />\n        </div>\n        <div class=\"form-control\">\n          <label for=\"price\">Prix*</label>\n          <div class=\"flex row gap-mini align-baseline\">\n            <span>$</span>\n            <input type=\"text\" name=\"price\" id=\"price\" placeholder=\"00.00\" v-model=\"pastry.price\" />\n          </div>\n        </div>\n        <div class=\"form-control\">\n          <label for=\"photo\">Photo</label>\n          <input type=\"text\" name=\"photo\" id=\"photo\" v-model=\"pastry.photo\" />\n        </div>\n      </div>\n      <div class=\"form-control\">\n        <label for=\"description\">Description*</label>\n        <textarea name=\"description\" id=\"description\" v-model=\"pastry.description\"></textarea>\n      </div>\n    </div>\n    <nav class=\"flex row align-baseline gap-normal\">\n      <router-link :to=\"{ name: 'pastry' }\">Annuler</router-link>\n      <button class=\"btn border-marron\" @click=\"deletePastry\">Supprimer</button>\n      <button class=\"btn btn-submit\" @click=\"updatePastry\">Enregistrer</button>\n    </nav>\n  </form>\n</template>\n\n<script>\nimport PastryDataService from '@/services/PastryDataService'\n\nexport default {\n\n  props: ['inventory', 'updateInv', 'removeInv', 'removeItem'],\n\n  data () {\n    return {\n      pastry: {},\n      id: parseInt(this.$route.params.id),\n      submitted: false\n      // message: null\n    }\n  },\n\n  methods: {\n    // update\n    updatePastry () {\n      PastryDataService.update(this.id, this.pastry).then(res => {\n        this.updateInv(this.pastryIndex, this.pastry)\n        // success message\n        this.submitted = true\n      })\n    },\n\n    // delete\n    deletePastry () {\n      PastryDataService.delete(this.id).then(res => {\n        // remove from cart\n        this.removeItem(this.pastry.name)\n        // delete from dom\n        this.removeInv(this.pastryIndex)\n        // redirect\n        this.$router.push({ name: 'home' })\n      })\n    }\n  },\n\n  computed: {\n    // find pastry index\n    pastryIndex () {\n      const index = this.inventory.findIndex((p) => {\n        return p.id === this.id\n      })\n      return index\n    }\n  },\n\n  // get data by id\n  mounted () {\n    PastryDataService.get(this.id).then(res => {\n      this.pastry = res.data\n    })\n  }\n\n}\n</script>\n"],"mappings":";AA6CA,OAAOA,iBAAgB,MAAO,8BAA6B;AAE3D,eAAe;EAEbC,KAAK,EAAE,CAAC,WAAW,EAAE,WAAW,EAAE,WAAW,EAAE,YAAY,CAAC;EAE5DC,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,MAAM,EAAE,CAAC,CAAC;MACVC,EAAE,EAAEC,QAAQ,CAAC,IAAI,CAACC,MAAM,CAACC,MAAM,CAACH,EAAE,CAAC;MACnCI,SAAS,EAAE;MACX;IACF;EACF,CAAC;;EAEDC,OAAO,EAAE;IACP;IACAC,YAAWA,CAAA,EAAK;MACdV,iBAAiB,CAACW,MAAM,CAAC,IAAI,CAACP,EAAE,EAAE,IAAI,CAACD,MAAM,CAAC,CAACS,IAAI,CAACC,GAAE,IAAK;QACzD,IAAI,CAACC,SAAS,CAAC,IAAI,CAACC,WAAW,EAAE,IAAI,CAACZ,MAAM;QAC5C;QACA,IAAI,CAACK,SAAQ,GAAI,IAAG;MACtB,CAAC;IACH,CAAC;IAED;IACAQ,YAAWA,CAAA,EAAK;MACdhB,iBAAiB,CAACiB,MAAM,CAAC,IAAI,CAACb,EAAE,CAAC,CAACQ,IAAI,CAACC,GAAE,IAAK;QAC5C;QACA,IAAI,CAACK,UAAU,CAAC,IAAI,CAACf,MAAM,CAACgB,IAAI;QAChC;QACA,IAAI,CAACC,SAAS,CAAC,IAAI,CAACL,WAAW;QAC/B;QACA,IAAI,CAACM,OAAO,CAACC,IAAI,CAAC;UAAEH,IAAI,EAAE;QAAO,CAAC;MACpC,CAAC;IACH;EACF,CAAC;EAEDI,QAAQ,EAAE;IACR;IACAR,WAAUA,CAAA,EAAK;MACb,MAAMS,KAAI,GAAI,IAAI,CAACC,SAAS,CAACC,SAAS,CAAEC,CAAC,IAAK;QAC5C,OAAOA,CAAC,CAACvB,EAAC,KAAM,I